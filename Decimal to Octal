public class DecimalToOctal {
    public static void main(String[] args) {
        int decimal = 156; 
        String octal = "";

        if (decimal == 0) {
            octal = "0";
        }

        while (decimal > 0) {
            int remainder = decimal % 8;
            octal = remainder + octal; 
            decimal = decimal / 8;
        }

        System.out.println("Octal: " + octal);
    }
}

O/P
Octal: 234
